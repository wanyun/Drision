    <link href="@Url.Content("~/Content/Site.css")" rel="stylesheet" 
        type="text/css" />
    <script src="@Url.Content("~/Scripts/jquery-1.7.1.min.js")" 
        type="text/javascript"></script>
</head>
<body>
    <div id="header">
        <h1><a href="/">White Tree</a></h1>
        <ul id="navlist">
            <li class="first"><a href="@Url.Content("~")" id="current">Home</a></li>
            <li><a href="@Url.Content("~/User/")">用户</a></li>
            <li><a href="@Url.Content("~/Role/")">角色</a></li>
            <li><a href="@Url.Content("~/Action/")">菜单</a></li>
            <li><a href="@Url.Content("~/Group/")">分组</a></li>
        </ul>        
